{
  "name": "automated-testing-framework",
  "version": "1.0.0",
  "description": "CI/CD testing with Cypress, Pact, and Pytest",
  "main": "index.js",
  "scripts": {
    "start": "npx serve demo-testing-framework/app -l 5500",
    "test:contract": "jest test/consumer.test.js --passWithNoTests",
    "test:verify": "pact-provider-verifier --provider-base-url=http://localhost:5000 --pact-urls=./pacts/consumer-provider.json"
  },
  "devDependencies": {
    "@pact-foundation/pact": "^15.0.1",
    "cypress": "^14.3.3",
    "jest": "^29.7.0",
    "serve": "^14.2.4",
    "wait-on": "^8.0.3"
  }
}
